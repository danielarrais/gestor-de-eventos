<p id="notice"><%= notice %></p>

<h1>Certificate Signatures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @certificate_signatures.each do |certificate_signature| %>
      <tr>
        <td><%= link_to 'Show', certificate_signature %></td>
        <td><%= link_to 'Edit', edit_certificate_signature_path(certificate_signature) %></td>
        <td><%= link_to 'Destroy', certificate_signature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Certificate Signature', new_certificate_signature_path %>
